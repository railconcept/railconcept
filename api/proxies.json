{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "callback": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/api/callback"
            },
            "backendUri": "http://localhost/api/actual_callback",
            "requestOverrides": {
                "backend.request.querystring.code": "",
                "backend.request.querystring._code": "{request.querystring.code}"
            }
        }
    }
}
